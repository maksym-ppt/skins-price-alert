{
  "version": 2,
  "builds": [
    {
      "src": "api/telegram.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/cron.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/telegram",
      "dest": "/api/telegram.ts"
    },
    {
      "src": "/api/cron",
      "dest": "/api/cron.ts"
    }
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 0 * * *"
    }
  ]
}
